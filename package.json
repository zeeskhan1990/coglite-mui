{
  "name": "coglite",
  "productName": "Coglite Desktop",
  "main": "out/desktop.js",
  "version": "0.0.1",
  "description": "app",
  "license": "MIT",
  "private": true,
  "author": "Coglite team",
  "repository": {
    "type": "git",
    "url": "https://github.com/coglite/coglite-desktop"
  },
  "homepage": "https://github.com",
  "scripts": {
    "build-storybook": "build-storybook",
    "clean": "rimraf .fusebox out dist coverage",
    "compile": "cross-env NODE_ENV=production ts-node fuse",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "coverage": "yarn test --coverage --no-cache",
    "dist": "electron-builder --mac --win --linux --ia32 --x64",
    "draft": "electron-builder -p always --draft --mac --win --linux --ia32 --x64",
    "format": "prettier --write **/*.ts && tslint -p . --fix",
    "lint": "tslint --project .",
    "pack": "electron-builder --dir",
    "updatedeps": "electron-builder install-app-deps",
    "precommit": "lint-staged",
    "prepush": "npm-run-all",
    "sanity": "npm-run-all clean typecheck lint test clean compile clean",
    "start": "ts-node fuse",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit --pretty"
  },
  "dependencies": {
    "@types/prop-types": "^15.5.2",
    "csx": "^8.5.0",
    "electron-is-dev": "0.3.0",
    "electron-log": "2.2.14",
    "electron-store": "1.3.0",
    "electron-updater": "2.20.1",
    "electron-window-state-manager": "0.3.2",
    "glamor": "2.20.40",
    "glamorous": "^4.11.4",
    "history": "^4.6.1",
    "material-components-web": "^0.31.0",
    "material-ui": "^1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.36",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "mobx-react-router": "^4.0.1",
    "mousetrap": "1.6.1",
    "popmotion": "8.1.10",
    "popmotion-react": "2.1.0",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "2.2.1",
    "rmwc": "^1.2.1",
    "typestyle": "^1.7.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.13",
    "@storybook/addon-links": "3.3.13",
    "@storybook/addon-storyshots": "3.3.13",
    "@storybook/react": "3.3.13",
    "@types/classnames": "^2.2.3",
    "@types/electron-is-dev": "0.3.0",
    "@types/electron-store": "1.2.0",
    "@types/history": "^4.6.2",
    "@types/jest": "22.1.3",
    "@types/mousetrap": "1.6.0",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "@types/react-router": "^4.0.22",
    "@types/react-test-renderer": "16.0.1",
    "@types/storybook__react": "3.0.7",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.0.0",
    "cross-env": "5.1.3",
    "csstips": "^0.2.2",
    "electron": "1.8.2",
    "electron-builder": "20.0.5",
    "fuse-box": "3.1.3",
    "husky": "0.14.3",
    "jest": "22.3.0",
    "jest-expect-contain-deep": "1.0.1",
    "lint-staged": "6.1.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "4.1.2",
    "prettier": "1.10.2",
    "react-powerplug": "0.1.4",
    "react-test-renderer": "16.2.0",
    "ts-jest": "22.0.4",
    "ts-loader": "3.5.0",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.8.0",
    "typescript": "2.7.2"
  },
  "build": {
    "appId": "com.example.coglite",
    "files": [
      "!src",
      "!docs",
      "!tsconfig.json",
      "!fuse.js",
      "!dev-app-update.yml",
      "!README.md",
      "!LICENSE",
      "!globals.d.ts"
    ],
    "linux": {
      "target": [
        "tar.gz"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    },
    "publish": {
      "owner": "coglite",
      "provider": "github",
      "repo": "coglite-desktop"
    }
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.ts",
      "\\.(css|less)$": "<rootDir>/test/mock-style.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "./node_modules",
      "./out",
      "./build",
      "./dist",
      "./test",
      "./docs",
      "\\.story.tsx$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false,
    "mapCoverage": true
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
